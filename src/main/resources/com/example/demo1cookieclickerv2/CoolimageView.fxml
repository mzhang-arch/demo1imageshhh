<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Accordion?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TitledPane?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns="http://javafx.com/javafx/25" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demo1cookieclickerv2.ImageController">
   <children>
      <Accordion fx:id="myAccordion" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0">
        <panes>
          <TitledPane animated="false" text="Yellow Coin">
            <content>
              <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                     <children>
                        <ImageView fx:id="coinImages" fitHeight="150.0" fitWidth="200.0" layoutX="201.0" layoutY="61.0" pickOnBounds="true" preserveRatio="true" />
                     </children>
                  </AnchorPane>
            </content>
          </TitledPane>
          <TitledPane animated="false" text="Red Coin">
            <content>
              <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                     <children>
                        <ImageView fx:id="CoinImagesv2" fitHeight="150.0" fitWidth="200.0" layoutX="223.0" layoutY="87.0" pickOnBounds="true" preserveRatio="true" />
                     </children>
                  </AnchorPane>
            </content>
          </TitledPane>
          <TitledPane animated="false" text="Grey Coin">
            <content>
              <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                     <children>
                        <ImageView fx:id="CoinImagev3" fitHeight="150.0" fitWidth="200.0" layoutX="224.0" layoutY="86.0" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@../../../../../../../../Downloads/Unbreakable.png" />
                           </image>
                        </ImageView>
                     </children>
                  </AnchorPane>
            </content>
          </TitledPane>
            <TitledPane animated="false" text="Nima">
               <content>
                  <AnchorPane prefHeight="200.0" prefWidth="200.0">
                     <children>
                        <ImageView fx:id="CoinImagev4" fitHeight="150.0" fitWidth="200.0" layoutX="184.0" layoutY="62.0" pickOnBounds="true" preserveRatio="true" />
                     </children>
                  </AnchorPane>
               </content>
            </TitledPane>
            <TitledPane animated="false" text="Lunacy">
               <content>
                  <AnchorPane prefHeight="200.0" prefWidth="200.0">
                     <children>
                        <ImageView fx:id="CoinImagev5" fitHeight="150.0" fitWidth="200.0" layoutX="187.0" layoutY="62.0" pickOnBounds="true" preserveRatio="true" />
                     </children>
                  </AnchorPane>
               </content>
            </TitledPane>
        </panes>
      </Accordion>
      <Button fx:id="addButton" mnemonicParsing="false" onAction="#handleAddButtonAction" prefHeight="25.0" prefWidth="600.0" text="Generate Image" />
      <Button fx:id="nextButton" mnemonicParsing="false" prefHeight="25.0" prefWidth="619.0" text="Next Image" />
      <Button fx:id="prevButton" mnemonicParsing="false" prefHeight="25.0" prefWidth="608.0" text="Previous Image" />
   </children>
</VBox>
